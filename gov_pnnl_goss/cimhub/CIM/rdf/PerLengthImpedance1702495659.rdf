@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM#> .

cim:PerLengthImpedance rdf:type rdfs:Class ;
    rdfs:label "PerLengthImpedance" ;
    rdfs:comment "Impedance that is uniform along the length of the equipment" ;
    rdfs:subClassOf cim:ConductancePerLength ;
    rdfs:subClassOf cim:ResistancePerLength ;
    rdfs:subClassOf cim:ReactancePerLength .

cim:ConductancePerLength rdf:type rdfs:Class ;
    rdfs:label "ConductancePerLength" ;
    rdfs:comment "The conductance per unit length of a portion of an asset." ;
    rdfs:subClassOf cim:PerLengthUnit .

cim:ResistancePerLength rdf:type rdfs:Class ;
    rdfs:label "ResistancePerLength" ;
    rdfs:comment "The resistance per unit length of a portion of an asset." ;
    rdfs:subClassOf cim:PerLengthUnit .

cim:ReactancePerLength rdf:type rdfs:Class ;
    rdfs:label "ReactancePerLength" ;
    rdfs:comment "The reactance per unit length of a portion of an asset." ;
    rdfs:subClassOf cim:PerLengthUnit .

cim:PerLengthUnit rdf:type rdfs:Class ;
    rdfs:label "PerLengthUnit" ;
    rdfs:comment "Unit value of something per unit length" .