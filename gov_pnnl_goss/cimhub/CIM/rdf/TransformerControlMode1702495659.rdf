@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/2012/CIM-schema-cim16#> .

cim:TransformerControlMode rdf:type cim:TransformerControlMode ;
    rdfs:label "Transformer Control Mode" ;
    cim:TransformerControlMode.modeName "AutomaticOnCount" ;
    cim:TransformerControlMode.modeValue "1" ;
    cim:TransformerControlMode.Descriptions "This mode means that the transformer switches automatically on the specified count setting" .
