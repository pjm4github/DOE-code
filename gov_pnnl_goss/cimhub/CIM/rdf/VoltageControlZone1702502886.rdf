@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM#> .

cim:VoltageControlZone rdf:type rdfs:Class ;
    rdfs:subClassOf cim:ControlArea ;
    rdfs:label "VoltageControlZone" ;
    rdfs:comment "A VoltageControlZone is a grouping of power system resources that are managed by a specific control center for the purpose of local voltage control." .

cim:VoltageControlZone.VoltageControlZone rdfs:subClassOf cim:VoltageControlZone ;
    rdfs:label "VoltageControlZone" .

cim:VoltageControlZone.ID rdf:type rdfs:Property ;
    rdfs:domain cim:VoltageControlZone ;
    rdfs:range xsd:ID ;
    rdfs:label "ID" ;
    rdfs:comment "A VoltageControlZone has a unique ID within a substation." .

cim:VoltageControlZone.name rdf:type rdfs:Property ;
    rdfs:domain cim:VoltageControlZone ;
    rdfs:range xsd:string ;
    rdfs:label "name" ;
    rdfs:comment "The name of the VoltageControlZone." .

cim:VoltageControlZone.MemberOf_Substation rdf:type rdfs:Property ;
    rdfs:domain cim:VoltageControlZone ;
    rdfs:range cim:Substation ;
    rdfs:label "MemberOf_Substation" ;
    rdfs:comment "The substation to which the VoltageControlZone belongs." .