@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM100#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

cim:PhaseTapChanger rdf:type cim:TapChanger ;
  rdfs:subClassOf cim:TapChanger ;
  cim:PhaseTapChanger.Ctmu cim:PhaseTapChanger_Ctmu ;
  cim:PhaseTapChanger.TransformerEnd cim:PhaseTapChanger_TransformerEnd .

cim:PhaseTapChanger_Ctmu rdf:type cim:PhaseTapChangerControl ;
  cim:PhaseTapChangerControl.CtmuMaxV cim:PhaseTapChangerControl_CtmuMaxV ;
  cim:PhaseTapChangerControl.CtmuMinV cim:PhaseTapChangerControl_CtmuMinV .

cim:PhaseTapChangerControl_CtmuMaxV rdf:type cim:Float ;
  cim:Float.value "100.0" .

cim:PhaseTapChangerControl_CtmuMinV rdf:type cim:Float ;
  cim:Float.value "90.0" .

cim:PhaseTapChanger_TransformerEnd rdf:type cim:TransformerEndInfo ;
  cim:TransformerEndInfo.TransformerEndVoltage cim:TransformerEndInfo_TransformerEndVoltage .

cim:TransformerEndInfo_TransformerEndVoltage rdf:type cim:PerCent ;
  cim:PerCent.value "105.0" .