    rdfs:range rdfs:Literal .

cim:RegulatingControlModeKind.highValueLimit rdf:type rdfs:DatatypeProperty ;
    rdfs:domain cim:RegulatingControlModeKind ;
    rdfs:range rdfs:Literal .

cim:RegulatingControlModeKind.normalValue rdf:type rdfs:DatatypeProperty ;
