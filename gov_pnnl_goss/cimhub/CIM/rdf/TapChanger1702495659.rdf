@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cim:TapChanger rdf:type cim:TapChanger ;
    cim:IdentifiedObject.mRID "TapChanger1"^^xsd:ID ;
    cim:IdentifiedObject.name "Example TapChanger"^^xsd:string ;
    cim:TapChanger.highStep 10 ;
    cim:TapChanger.lowStep 1 ;
    cim:TapChanger.normalStep 5 ;
    cim:TapChanger.neutralStep 3 ;
    cim:TapChanger.stepVoltageIncrement 2.5 .

cim:TapChangerControl rdf:type cim:TapChangerControl ;
    cim:IdentifiedObject.mRID "TapChangerControl1"^^xsd:ID ;
    cim:IdentifiedObject.name "Example TapChanger Control"^^xsd:string ;
    cim:TapChangerControl.ltcFlag true ;
    cim:TapChangerControl.lineDropCompensationFlag false ;
    cim:TapChangerControl.lineDropR X'ABCD' .