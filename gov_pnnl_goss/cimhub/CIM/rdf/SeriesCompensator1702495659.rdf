@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cim:SeriesCompensator rdf:type cim:SeriesCompensator ;
    cim:IdentifiedObject.name "SeriesCompensator1" ;
    cim:PowerSystemResource.equipmentContainer cim:Substation1 ;
    cim:SeriesDevice.R R1 ;
    cim:SeriesDevice.X X1 ;
    cim:Equipment.nominalVoltage "110000"^^xsd:double .

cim:Substation1 rdf:type cim:Substation ;
    cim:IdentifiedObject.name "Substation1" .

cim:R1 rdf:type cim:Resistor ;
    cim:IdentifiedObject.name "R1" ;
    cim:ConductingEquipment.BaseVoltage "110000"^^xsd:double .

cim:X1 rdf:type cim:Reactance ;
    cim:IdentifiedObject.name "X1" ;
    cim:ConductingEquipment.BaseVoltage "110000"^^xsd:double .