@prefix cim: <http://iec.ch/TC57/2013/CIM-schema-cim16#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

cim:Connector rdf:type cim:ConnectivityNode;
    cim:IdentifiedObject.mRID "123456";
    cim:IdentifiedObject.name "Connector1";
    cim:ConnectivityNode.TopologicalNode cim:TopologicalNode1;
    cim:ConnectivityNode.ConnectivityNodeContainer cim:Substation1.

cim:ConnectivityNode rdf:type cim:IdentifiedObject;
    rdfs:subClassOf cim:PowerSystemResource;
    cim:IdentifiedObject.mRID "123456";
    cim:IdentifiedObject.name "Node1".

cim:Substation1 rdf:type cim:Substation;
    cim:IdentifiedObject.mRID "789012";
    cim:IdentifiedObject.name "Substation1".

cim:TopologicalNode1 rdf:type cim:TopologicalNode;
    cim:IdentifiedObject.mRID "345678";
    cim:IdentifiedObject.name "Node1".