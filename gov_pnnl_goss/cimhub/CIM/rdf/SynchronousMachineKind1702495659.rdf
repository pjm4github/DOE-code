@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cim:SynchronousMachineKind rdf:type rdfs:Class ;
    rdfs:subClassOf cim:RotatingMachineKind .

cim:HydroTurbine rdf:type rdfs:Class ;
    rdfs:subClassOf cim:SynchronousMachineKind .

cim:SteamTurbine rdf:type rdfs:Class ;
    rdfs:subClassOf cim:SynchronousMachineKind .

cim:SynchronousMotor rdf:type rdfs:Class ;
    rdfs:subClassOf cim:SynchronousMachineKind .

cim:SynchronousGenerator rdf:type rdfs:Class ;
    rdfs:subClassOf cim:SynchronousMachineKind .