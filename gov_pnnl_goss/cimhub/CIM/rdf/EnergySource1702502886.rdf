@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM100#> .
@prefix ex: <http://example.org#> .

ex:EnergySource rdf:type cim:EnergySource ;
    cim:EnergySource.energyType "Renewable" ;
    cim:IdentifiedObject.name "Solar Panel 1" ;
    cim:IdentifiedObject.description "Solar panel located on rooftop" ;
    cim:Equipment.Equipment.container ex:Substation1 .

ex:SolarPanel1 rdf:type cim:SolarPanel ;
    cim:SolarPanel.ratedCapacity "250" ;
    cim:Equipment.ratedVoltage "24" .

ex:Substation1 rdf:type cim:Substation ;
    cim:IdentifiedObject.name "Substation 1" ;
    cim:IdentifiedObject.description "Primary substation receiving power from solar panels" .