@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix example: <http://example.com/fuse#> .

example:Fuse rdf:type cim:Fuse .
example:Fuse cim:Equipment.EquipmentContainer example:FuseContainer .
example:Fuse cim:Equipment.EquipmentPowerSystemResources example:FusePSR1 .
example:Fuse cim:IdentifiedObject.mRID "example.Fuse1" .
example:Fuse cim:IdentifiedObject.name "Fuse1" .

example:FuseContainer rdf:type cim:EquipmentContainer .
example:FuseContainer cim:IdentifiedObject.mRID "example.FuseContainer1" .
example:FuseContainer cim:IdentifiedObject.name "FuseContainer1" .

example:FusePSR1 rdf:type cim:PowerSystemResource .
example:FusePSR1 cim:IdentifiedObject.mRID "example.FusePSR1" .
example:FusePSR1 cim:IdentifiedObject.name "FusePSR1" .