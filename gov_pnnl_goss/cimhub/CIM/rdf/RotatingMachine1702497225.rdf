@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM100#> .
@prefix ex: <http://example.com/rotatingmachine#> .

ex:RotatingMachine rdf:type cim:RotatingMachine ;
  cim:IdentifiedObject.mRID "1" ;
  cim:IdentifiedObject.name "RotatingMachine1" ;
  cim:IdentifiedObject.description "Example Rotating Machine" ;
  cim:RotatingMachine.ratedS 100.0 ;
  cim:RotatingMachine.ratedPowerFactor 0.9 ;
  cim:RotatingMachine.statorType cim:StatorType_SalientPole ;
  cim:RotatingMachine.rotorType cim:RotorType_Cage.

ex:Terminal rdf:type cim:Terminal ;
  cim:IdentifiedObject.mRID "2" ;
  cim:IdentifiedObject.name "Terminal1" .

ex:Measurement rdf:type cim:Measurement ;
  cim:IdentifiedObject.mRID "3" ;
  cim:IdentifiedObject.name "VoltageMeasurement" .

ex:Terminal cim:Terminal.ConductingEquipment ex:RotatingMachine.

ex:Measurement cim:Measurement.Terminal ex:Terminal .