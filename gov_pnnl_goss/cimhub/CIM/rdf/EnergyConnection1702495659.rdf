@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/2013/CIM-schema-cim16#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cim:EnergyConnection rdf:type cim:Equipment ;
    cim:Equipment.EquipmentContainer cim:Substation_1 ;
    cim:Equipment.normalState cim:OperationalLimitKindNormal ;
    cim:Equipment.aggregate false ;
    cim:PowerSystemResource.AssetRoles cim:AssetRoleType_Equipment ;
    cim:PowerSystemResource.AssetRoles cim:AssetRoleType_PowerSystemResource .

cim:Substation_1 rdf:type cim:Substation ;
    cim:IdentifiedObject.name "ExampleSubstation" ;
    cim:Substation.Region cim:SubGeographicalRegion_1 .

cim:OperationalLimitKindNormal rdf:type cim:OperationalLimitKind .

cim:AssetRoleType_Equipment rdf:type cim:AssetRoleType .

cim:AssetRoleType_PowerSystemResource rdf:type cim:AssetRoleType .

cim:SubGeographicalRegion_1 rdf:type cim:SubGeographicalRegion ;
    cim:IdentifiedObject.name "ExampleRegion" ;
    cim:SubGeographicalRegion.Region cim:GeographicalRegion_1 .

cim:GeographicalRegion_1 rdf:type cim:GeographicalRegion ;
    cim:IdentifiedObject.name "ExampleGeographicalRegion" .