@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM#> .
@prefix ex: <http://example.com#> .

ex:Sectionaliser rdf:type cim:Sectionaliser ;
    cim:Equipment.EquipmentContainer "exampleEquipmentContainer" ;
    cim:ConductingEquipment.BaseVoltage "exampleBaseVoltage" ;
    cim:IdentifiedObject.mRID "exampleSectionaliser" ;
    cim:IdentifiedObject.name "Example Sectionaliser" ;
    cim:ConductingEquipment.phases "ABC" ;
    cim:Switch.normalOpen true .

ex:exampleEquipmentContainer rdf:type cim:EquipmentContainer .

ex:exampleBaseVoltage rdf:type cim:BaseVoltage ;
    cim:BaseVoltage.nominalVoltage "11000" .