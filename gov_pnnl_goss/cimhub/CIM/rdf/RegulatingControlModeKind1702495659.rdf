@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/2013/CIM-schema-cim16#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cim:RegulatingControlModeKind rdf:type rdfs:Class ;
    rdfs:subClassOf cim:Enumeration ;
    rdfs:label "RegulatingControlModeKind"^^xsd:string ;
    rdfs:comment "RegulatingControlModeKind is an enumerated attribute type for distinguishing the different kinds of control performance."^^xsd:string .

cim:RegulatingControlModeKind.NORMAL cim:EnumValue ;
    rdfs:label "normal"^^xsd:string ;
    rdfs:comment "Control to maintain the controlled quantity at the desired value."^^xsd:string .

cim:RegulatingControlModeKind.DROOP cim:EnumValue ;
    rdfs:label "droop"^^xsd:string ;
    rdfs:comment "Control where the controlled quantity is varied in proportion to the difference between the actual and desired values."^^xsd:string .

cim:RegulatingControlModeKind.AUTOMATIC cim:EnumValue ;
    rdfs:label "automatic"^^xsd:string ;
    rdfs:comment "Control where the set-point is automatically modified based on measurements or operating conditions."^^xsd:string .

cim:RegulatingControlModeKind.MANUAL cim:EnumValue ;
    rdfs:label "manual"^^xsd:string ;
    rdfs:comment "Control where the set-point is manually adjusted."^^xsd:string .
