@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/2010/CIM-schema-cim15#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cim:ReactiveCapabilityCurve rdf:type cim:Curve;
    cim:IdentifiedObject.name "ExampleReactiveCapabilityCurve1";

cim:Curve.title "Example Reactive Capability Curve 1";
    cim:Curve.xAxisUnits "MVAr";
    cim:Curve.y1AxisUnits "MW";
    cim:Curve.y2AxisUnits "MW";
    cim:Curve.x1Unit "pu";
    cim:Curve.x2Unit "pu";

cim:CurveData
    rdf:type cim:CurveData;
    cim:CurveData.xvalue  0.0;
    cim:CurveData.y1value -100.0;
    cim:CurveData.y2value 0.0;
    cim:CurveData.curveIndex 1;

cim:CurveData
    rdf:type cim:CurveData;
    cim:CurveData.xvalue  1.0;
    cim:CurveData.y1value 0.0;
    cim:CurveData.y2value 100.0;
    cim:CurveData.curveIndex 1.