@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM#> .
@prefix ex: <http://example.com#> .

ex:PhaseTapChangerSymmetrical rdf:type cim:PhaseTapChangerSymmetrical ;
    cim:PowerTransformer ex:powerTransformer1 ;
    cim:nomU ex:nomUValue ;
    cim:neutralStep ex:neutralStepValue ;
    cim:step ex:stepValue .

ex:powerTransformer1 rdf:type cim:PowerTransformer ;
    cim:operatingMode "HV" ;
    cim:ratedS ex:ratedSValue .

ex:nomUValue rdf:type cim:Voltage ;
    cim:multiplier "k" ;
    cim:value "110" ;
    cim:unit "V" .

ex:neutralStepValue rdf:type cim:Integer ;
    cim:value "1" .

ex:stepValue rdf:type cim:Integer ;
    cim:value "3" .

ex:ratedSValue rdf:type cim:ApparentPower ;
    cim:multiplier "M" ;
    cim:value "100" ;
    cim:unit "VA" .