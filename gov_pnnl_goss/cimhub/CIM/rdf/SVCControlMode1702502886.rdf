@prefix cim: <http://iec.ch/TC57/2013/CIM-schema-cim16#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

cim:SVCControlMode rdf:type rdfs:Class ;
    rdfs:subClassOf cim:ControlMode ;
    rdfs:comment "A voltage control mode employed by a static var compensator (SVC)" .

cim:VoltageControlMode rdf:type rdfs:Class ;
    rdfs:subClassOf cim:SVCControlMode ;
    rdfs:comment "A voltage control mode" .