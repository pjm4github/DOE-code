@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM100#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex: <http://example.org#> .

ex:Jumper rdf:type cim:ConductingEquipment ;
    cim:Equipment.EquipmentContainer ex:VoltageLevel1 ;
    cim:PowerSystemResource.AssetRoles ex:Conductor1 ;
    cim:IdentifiedObject.mRID "Jumper1" ;
    cim:IdentifiedObject.name "Jumper 1" .

ex:VoltageLevel1 rdf:type cim:VoltageLevel ;
    cim:VoltageLevel.LowVoltageLimit 1000 ;
    cim:VoltageLevel.HighVoltageLimit 1100 ;
    cim:VoltageLevel.Substation ex:Substation1 ;
    cim:IdentifiedObject.mRID "VL1" ;
    cim:IdentifiedObject.name "Voltage Level 1" .

ex:Substation1 rdf:type cim:Substation ;
    cim:Substation.Region ex:Region1 ;
    cim:IdentifiedObject.mRID "SS1" ;
    cim:IdentifiedObject.name "Substation 1" .

ex:Region1 rdf:type cim:SubGeographicalRegion ;
    cim:SubGeographicalRegion.Region ex:Country1 ;
    cim:IdentifiedObject.mRID "Reg1" ;
    cim:IdentifiedObject.name "Region 1" .

ex:Country1 rdf:type cim:Country ;
    cim:IdentifiedObject.mRID "C1" ;
    cim:IdentifiedObject.name "Country 1" .