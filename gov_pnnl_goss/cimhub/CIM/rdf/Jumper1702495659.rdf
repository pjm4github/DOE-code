@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM100#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.com/cim#> .

ex:Jumper rdf:type cim:ConductingEquipment ;
    cim:Equipment.EquipmentContainer ex:Substation1 ;
    cim:Equipment.aggregate ex:Line1 ;
    cim:IdentifiedObject.name "Jumper1" ;
    cim:ConductingEquipment.BaseVoltage ex:BaseVoltage1 ;
    cim:ConductingEquipment.phases "ABC" ;
    cim:ConductingEquipment.PowerSystemResource.Location ex:Location1 .

ex:Line1 rdf:type cim:ACLineSegment ;
    cim:ConductingEquipment.BaseVoltage ex:BaseVoltage1 ;
    cim:IdentifiedObject.name "Line1" ;
    cim:PowerSystemResource.Location ex:Location1 .

ex:Substation1 rdf:type cim:Substation ;
    cim:IdentifiedObject.name "Substation1" ;
    cim:Substation.Region ex:Region1 .

ex:BaseVoltage1 rdf:type cim:BaseVoltage ;
    cim:BaseVoltage.nominalVoltage "11000"^^xsd:float ;
    cim:BaseVoltage.nominalVoltage "V" .

ex:Location1 rdf:type cim:Location ;
    cim:IdentifiedObject.name "Location1" ;
    cim:PositionPoint.CoordinateSystem ex:CoordinateSystem1 ;
    cim:PositionPoint.LocationIdentifier "12345" ;
    cim:PositionPoint.xPosition "10.0"^^xsd:float ;
    cim:PositionPoint.yPosition "20.0"^^xsd:float .

ex:Region1 rdf:type cim:SubGeographicalRegion ;
    cim:IdentifiedObject.name "Region1" ;
    cim:SubGeographicalRegion.Region ex:GeographicalRegion1 .

ex:GeographicalRegion1 rdf:type cim:GeographicalRegion ;
    cim:IdentifiedObject.name "GeographicalRegion1" .

ex:CoordinateSystem1 rdf:type cim:CoordinateSystem ;
    cim:IdentifiedObject.name "CoordinateSystem1" .