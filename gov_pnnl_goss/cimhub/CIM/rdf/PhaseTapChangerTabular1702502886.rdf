@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM100#> .
@prefix ex: <http://example.com/CIMModel#> .

ex:PhaseTapChangerTabular rdf:type cim:PhaseTapChangerTabular ;
    cim:PowerTransformerInfo ex:powerTransformerInfo1 ;
    cim:TapChangerControl ex:tapChangerControl1 ;
    cim:TapChangerInfo ex:tapChangerInfo1 ;
    cim:tapPosition ex:tapPosition1 .

ex:powerTransformerInfo1 rdf:type cim:PowerTransformerInfo ;
    cim:IdentifiedObject.name "PT Info 1" .

ex:tapPosition1 rdf:type cim:TapPosition ;
    cim:IdentifiedObject.name "Tap Position 1" ;
    cim:TapPosition.step 1 .

ex:tapChangerInfo1 rdf:type cim:TapChangerInfo ;
    cim:IdentifiedObject.name "Tap Changer Info 1" .

ex:tapChangerControl1 rdf:type cim:TapChangerControl ;
    cim:IdentifiedObject.name "Tap Changer Control 1" .