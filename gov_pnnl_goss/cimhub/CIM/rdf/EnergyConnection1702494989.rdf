@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/2013/CIM-schema-cim16#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cim:EnergyConnection rdf:type cim:ConductingEquipment ;
    cim:Equipment.EquipmentContainer cim:Substation3 ;
    cim:ConductingEquipment.BaseVoltage cim:BaseVoltage2 ;
    cim:ConductingEquipment.Terminals cim:Terminal001 .

cim:Substation3 rdf:type cim:Substation ;
    cim:IdentifiedObject.name "Substation 3"^^xsd:string .

cim:BaseVoltage2 rdf:type cim:BaseVoltage ;
    cim:IdentifiedObject.name "HV 132kV"^^xsd:string ;
    cim:BaseVoltage.nominalVoltage "132000"^^xsd:double .

cim:Terminal001 rdf:type cim:Terminal ;
    cim:IdentifiedObject.name "Terminal 001"^^xsd:string ;
    cim:Terminal.ConnectivityNode cim:ConnectivityNode001 .

cim:ConnectivityNode001 rdf:type cim:ConnectivityNode ;
    cim:IdentifiedObject.name "Connectivity Node 001"^^xsd:string .