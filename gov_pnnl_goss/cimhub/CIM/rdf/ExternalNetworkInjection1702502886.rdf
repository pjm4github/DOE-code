@prefix cim: <http://iec.ch/TC57/2013/CIM-schema-cim16#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cim:ExternalNetworkInjection rdf:type cim:Injection ;
    cim:PowerSystemResource.PSRType cim:PSRTypeKind ;
    cim:PowerSystemResource.equipmentContainer <http://iec.ch/TC57/2013/CIM-schema-cim16#Substation_MySubstation> ;
    cim:IdentifiedObject.mRID "MyExternalInjectionMRID" ;
    cim:IdentifiedObject.name "MyExternalInjection" ;
    cim:Injection.grossRating 100.0 ;
    cim:Injection.netRating 90.0 ;
    cim:Equipment.aggregate true ;
    cim:Equipment.normallyInService true ;
    cim:Equipment.AssetInfo <http://iec.ch/TC57/2013/CIM-schema-cim16#AssetInfo_MyAssetInfo> .

cim:Substation_MySubstation rdf:type cim:Substation ;
    cim:IdentifiedObject.mRID "MySubstationMRID" ;
    cim:IdentifiedObject.name "MySubstation" .

cim:AssetInfo_MyAssetInfo rdf:type cim:AssetInfo ;
    cim:IdentifiedObject.mRID "MyAssetInfoMRID" ;
    cim:IdentifiedObject.name "MyAssetInfo" .