@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://example.com/CIMModel#> .

base:Switch1 a cim:Switch ;
    cim:IdentifiedObject.mRID "Switch1" ;
    cim:IdentifiedObject.name "Switch 1" ;
    cim:Equipment.EquipmentContainer base:Substation1 ;
    cim:ConductingEquipment.BaseVoltage base:VoltageLevel1 .

base:VoltageLevel1 a cim:VoltageLevel ;
    cim:IdentifiedObject.mRID "VoltageLevel1" ;
    cim:IdentifiedObject.name "Voltage Level 1" ;
    cim:VoltageLevel.Substation base:Substation1 ;
    cim:VoltageLevel.nominalVoltage "11000" .

base:Substation1 a cim:Substation ;
    cim:IdentifiedObject.mRID "Substation1" ;
    cim:IdentifiedObject.name "Substation 1" .
