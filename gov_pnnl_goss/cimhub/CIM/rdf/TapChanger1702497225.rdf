@prefix cim: <http://iec.ch/TC57/CIM100#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.com/example#> .

ex:TapChanger rdf:type cim:TapChanger ;
    cim:Equipment.EquipmentContainer "ExampleContainer" ;
    cim:IdentifiedObject.mRID "ExampleTapChangerID" ;
    cim:IdentifiedObject.name "ExampleTapChanger" ;
    cim:TapChanger.stepVoltageIncrement 1.0 ;
    cim:TapChanger.tculControlYVc "ExampleControlMode" ;
    cim:TapChanger.tcTapChgTable ex:ExampleTapChangerTableID .

ex:ExampleTapChangerTableID rdf:type cim:TapChangerTable ;
    cim:IdentifiedObject.mRID "ExampleTapChangerTableID" ;
    cim:IdentifiedObject.name "ExampleTapChangerTable" ;
    cim:TapChangerTable.highStep 10 ;
    cim:TapChangerTable.initialDelay 5.0 ;
    cim:TapChangerTable.stepPhaseIncrement 1.0 ;
    cim:TapChangerTable.stepVoltageIncrement 1.0 ;
    cim:TapChangerTable.subsequentDelay 2.0 .