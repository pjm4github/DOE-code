@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cim:Junction rdf:type rdfs:Class ;
    rdfs:subClassOf cim:Equipment ;

    cim:Junction.ElectricalJunction rdf:type rdfs:Class ;
        rdfs:subClassOf cim:Junction ;

        cim:Junction.ElectricalJunction.ElectricalNode rdf:type rdfs:Class ;
            rdfs:subClassOf cim:Junction.ElectricalJunction .

cim:Junction.ElectricalJunction a cim:Class .

cim:Junction.ElectricalJunction.ElectricalNode a cim:Class .