@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cim:SwitchSchedule rdf:type rdfs:Class ;
    rdfs:subClassOf cim:Schedule.

cim:SwitchSchedule.ScheduleMember rdf:type rdfs:Class ;
    rdfs:subClassOf cim:IdentifiedObject.

cim:SwitchSchedule.scheduleBy rdf:type rdf:Property ;
    rdfs:domain cim:SwitchSchedule ;
    rdfs:range cim:Switch.

cim:Switch.rdf:type rdfs:Class ;
    rdfs:subClassOf cim:Equipment.

cim:Switch rdf:type rdfs:Class ;
    rdfs:subClassOf cim:ConductingEquipment.

cim:Switch.state rdf:type rdf:Property ;
    rdfs:domain cim:Switch ;
    rdfs:range cim:SwitchStateKind.

cim:SwitchStateKind rdf:type rdfs:Class ;
    rdfs:subClassOf rdfs:Literal.

cim:SwitchSchedule1 rdf:type cim:SwitchSchedule ;
    cim:SwitchSchedule.scheduleBy cim:Switch1.

cim:Switch1 rdf:type cim:Switch ;
    cim:Switch.state cim:Closed.

cim:SwitchSchedule2 rdf:type cim:SwitchSchedule ;
    cim:SwitchSchedule.scheduleBy cim:Switch2.

cim:Switch2 rdf:type cim:Switch ;
    cim:Switch.state cim:Open.