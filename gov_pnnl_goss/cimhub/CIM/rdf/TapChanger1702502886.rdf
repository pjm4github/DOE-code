@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/2013/CIM-schema-cim16#> .

<http://iec.ch/TC57/2013/CIM-schema-cim16#TapChanger>
  a rdfs:Class ;
  rdfs:label "TapChanger"@en ;
  rdfs:comment "A tap changer is a device fitted to power transformers for voltage control during the operation of the power system."@en ;
  rdfs:subClassOf cim:RegulatingControl .

<http://iec.ch/TC57/2013/CIM-schema-cim16#TapChanger.normalStep@1>
  a rdfs:Property ;
  rdfs:label "normalStep"@en ;
  rdfs:comment "The tap step position (e.g. "-2" means the 3rd position above the 0 position)."@en ;
  rdfs:domain cim:TapChanger .