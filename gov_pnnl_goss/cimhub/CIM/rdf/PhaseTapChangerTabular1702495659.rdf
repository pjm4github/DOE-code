@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cim:PhaseTapChangerTabular rdf:type cim:TapChanger ;
    cim:TapChanger.highStep 10 ;
    cim:TapChanger.initialDelay 1.0 ;
    cim:TapChanger.normalStep 5 ;
    cim:PhaseTapChangerTabular.stepPhaseShifter [
        rdf:type cim:PhaseTapChangerTable ;
        cim:PhaseTapChangerTable.step [ 
            a cim:TapChangerTablePoint ;
            cim:TapChangerTablePoint.position 1 ;
            cim:TapChangerTablePoint.r 1.0 ;
            cim:TapChangerTablePoint.x 0.2 ;
        ] ;
        cim:PhaseTapChangerTable.step [
            a cim:TapChangerTablePoint ;
            cim:TapChangerTablePoint.position 2 ;
            cim:TapChangerTablePoint.r 0.9 ;
            cim:TapChangerTablePoint.x 0.15 ;
        ] ;
    ] .
