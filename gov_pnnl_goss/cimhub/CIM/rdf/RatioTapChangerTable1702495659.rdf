  cim:RatioTapChangerTable.tabDelay: "0.1"^^xsd:float;
  cim:RatioTapChangerTable.tabDelayTolerance: "0.05"^^xsd:float;
  cim:RatioTapChangerTable.highStep: "16"^^xsd:integer;
  cim:RatioTapChangerTable.lowStep: "1"^^xsd:integer;
  cim:RatioTapChangerTable.RatioTapChanger: cim:RatioTapChanger;
  cim:RatioTapChangerTable.TapChangerControl: cim:TapChangerControl.

cim:RatioTapChanger rdf:type cim:TapChanger.

cim:TapChangerControl rdf:type cim:TapChangerControl.