@prefix cim: <http://iec.ch/TC57/CIM100#> .
@prefix ex: <http://example.org/CIMModel#> .

ex:EnergyConsumerA rdf:type cim:EnergyConsumer ;
    cim:IdentifiedObject.mRID "123456" ;
    cim:IdentifiedObject.name "Consumer1" ;
    cim:IdentifiedObject.description "Residential Energy Consumer" ;
    cim:ConductingEquipment.BaseVoltage "11000" ;
    cim:EnergyConsumer.p "1500" ;
    cim:EnergyConsumer.q "500" ;
    cim:EnergyConsumer.pfixed "1000" ;
    cim:EnergyConsumer.qfixed "300" ;
    cim:EnergyConsumer.LoadResponseCharacteristic.exponentModel "1" .

ex:EnergyConsumerB rdf:type cim:EnergyConsumer ;
