@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/2013/CIM-schema-cim16#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cim:EnergySourcePhase rdf:type rdfs:Class ;
    rdfs:subClassOf cim:ConductingEquipmentPhase .

cim:EnergySourcePhase.ratedS rdf:type rdfs:Property ,
        cim:BasePowerProperty ;
    rdfs:label "The active power rating of the energy source phase."@en ;
    rdfs:comment "The phase power rating of the energy source phase. For a multi-phase energy source, this would be the per-phase rating."@en ;
    rdfs:domain cim:EnergySourcePhase ;
    rdfs:range xsd:float .

cim:EnergySourcePhase.ratedU rdf:type rdfs:Property ,
        cim:BaseVoltageProperty ;
    rdfs:label "The rated voltage of the energy source phase."@en ;
    rdfs:comment "The phase voltage rating of the energy source phase. For a multi-phase energy source, this would be the line-line voltage."@en ;
    rdfs:domain cim:EnergySourcePhase ;
    rdfs:range xsd:float .

cim:EnergySourcePhase.ratedS 123.45 .
cim:EnergySourcePhase.ratedU 11000.0 .