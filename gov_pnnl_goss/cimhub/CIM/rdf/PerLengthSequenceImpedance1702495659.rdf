@prefix cim: <http://iec.ch/TC57/2019/CIM-schema-cim17#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

cim:PerLengthSequenceImpedance rdf:type cim:EquivalentEquipment ;
    cim:BaseVoltage "132000"^^<http://iec.ch/TC57/2019/CIM-schema-cim17#Voltage> ;
    cim:Length "1.0"^^<http://iec.ch/TC57/2019/CIM-schema-cim17#Length> ;
    cim:ConductorCount "3"^^<http://www.w3.org/2001/XMLSchema#integer> ;
    cim:SequenceImpedance rdfs:resource="http://example.com/sequenceImpedance1" ;
    cim:EquivalentNetwork rdf:resource="http://example.com/network1" .

cim:SequenceImpedance rdf:type cim:PositiveSequenceImpedance ;
    cim:value "0.05+0.2i"^^<http://iec.ch/TC57/2019/CIM-schema-cim17#ComplexType> .

cim:EquivalentNetwork rdf:type cim:Network ;
    cim:baseVoltage "132000"^^<http://iec.ch/TC57/2019/CIM-schema-cim17#Voltage> ;
    cim:Location "Country1" ;
    cim:Substation "Substation1" .