@prefix cim: <http://iec.ch/TC57/CIM#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

cim:EnergySource rdf:type cim:EnergySource ;
    cim:IdentifiedObject.name "Wind Farm" ;
    cim:PowerSystemResource.Location "123.456, 789.012" ;
    cim:EnergySource.energyType "Wind" ;
    cim:Equipment.aggregate "true" ;
    cim:Equipment.ratedS "1000000" ;
    cim:Equipment.OperationalLimitSet "OperationalLimitSet001" .

cim:OperationalLimitSet001 rdf:type cim:OperationalLimitSet ;
    cim:IdentifiedObject.name "Wind Farm Operational Limit Set" ;
    cim:OperationalLimitSet.OperationalLimit "OperationalLimit001", "OperationalLimit002" .

cim:OperationalLimit001 rdf:type cim:OperationalLimit ;
    cim:IdentifiedObject.name "Maximum Active Power" ;
    cim:OperationalLimit.value "900000" ;
    cim:OperationalLimit.unit "W" .

cim:OperationalLimit002 rdf:type cim:OperationalLimit ;
    cim:IdentifiedObject.name "Minimum Active Power" ;
    cim:OperationalLimit.value "100000" ;
    cim:OperationalLimit.unit "W" .