@prefix cim: <http://iec.ch/TC57/2013/CIM-schema-cim16#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/> .

ex:ExternalNetworkInjection rdf:type cim:EnergySource ;
                           cim:IdentifiedObject.name "External Network Injection Point" ;
                           cim:PowerSystemResource.Location "ExternalNetworkLocation" ;
                           cim:EnergyConnection.EnergyConnectionAction "generate" ;
                           cim:PowerSystemResource.AssetRoles cim:AssetRoles.AssetRoles_ExternalNetworkInjection .

ex:ExternalNetworkLocation rdf:type cim:Location ;
                         cim:IdentifiedObject.name "External Network Location" ;
                         cim:Location.CoordinateInfo "latitude:40.7128, longitude:-74.0060"^^xsd:string .
