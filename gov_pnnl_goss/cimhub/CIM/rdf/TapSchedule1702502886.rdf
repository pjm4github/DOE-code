    rdfs:subClassOf cim:SeasonDayTypeSchedule.SwitchingSchedule.Switch.SwitchSchedule .

cim:TapSchedule.SwitchingSchedule.Switch.SwitchSchedule.SwitchAction rdf:type rdfs:Class ;
    rdfs:subClassOf cim:SeasonDayTypeSchedule.SwitchingSchedule.Switch.SwitchSchedule.SwitchAction .

