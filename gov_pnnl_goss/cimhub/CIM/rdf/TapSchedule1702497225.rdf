@prefix cim:   <http://iec.ch/TC57/CIM#> .

cim:TapSchedule rdf:type cim:TapSchedule ;
    cim:IdentifiedObject.mRID "1" ;
    cim:IdentifiedObject.name "TapSchedule1" ;
    cim:IdentifiedObject.description "Example TapSchedule" ;
    cim:TapSchedule.TapChangerControl cim:TapChangerControl1 .

cim:TapChangerControl1 rdf:type cim:TapChangerControl ;
    cim:IdentifiedObject.mRID "2" ;
    cim:IdentifiedObject.name "TapChangerControl1" ;
    cim:IdentifiedObject.description "Example TapChangerControl" ;
    cim:TapChangerControl.highStep 10 ;
    cim:TapChangerControl.lowStep 1 .

