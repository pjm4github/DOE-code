@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix example: <http://example.com/CIM#> .

example:Disconnector rdf:type cim:Disconnector ;
    cim:Equipment.EquipmentContainer example:Substation1 ;
    cim:ConductingEquipment.BaseVoltage example:BaseVoltage1 ;
    cim:IdentifiedObject.mRID "123456789" ;
    cim:IdentifiedObject.name "Disconnect001" .

example:Substation1 rdf:type cim:Substation ;
    cim:Equipment.EquipmentContainer example:Substation ;
    cim:IdentifiedObject.mRID "987654321" ;
    cim:IdentifiedObject.name "MainSubstation" .

example:BaseVoltage1 rdf:type cim:BaseVoltage ;
    cim:BaseVoltage.nominalVoltage "110000" .
