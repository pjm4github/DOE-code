@prefix cim: <http://iec.ch/TC57/2013/CIM-schema-cim16#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

cim:BusbarSection rdf:type cim:ConductingEquipment ;
    cim:ConductingEquipment.BaseVoltage "20000"^^xsd:double ;
    cim:Equipment.EquipmentContainer <http://example.com/Network/BusbarSection1> ;
    cim:IdentifiedObject.aliasName "BusbarSection1" ;
    cim:IdentifiedObject.description "This is a busbar section in the electrical network" .

<http://example.com/Network/BusbarSection1> rdf:type cim:VoltageLevel ;
    cim:VoltageLevel.Substation <http://example.com/Substation/Substation1> ;
    cim:VoltageLevel.nominalVoltage "20000"^^xsd:double ;
    cim:IdentifiedObject.mrid "12345-abc-67890-def" ;
    cim:IdentifiedObject.name "BusbarSection1" .

<http://example.com/Substation/Substation1> rdf:type cim:Substation ;
    cim:Substation.Region <http://example.com/Region/Region1> ;
    cim:IdentifiedObject.mrid "67890-def-12345-abc" ;
    cim:IdentifiedObject.name "Substation1" .

<http://example.com/Region/Region1> rdf:type cim:SubGeographicalRegion ;
    cim:IdentifiedObject.mrid "xyz-456-789-abc" ;
    cim:IdentifiedObject.name "Region1" .