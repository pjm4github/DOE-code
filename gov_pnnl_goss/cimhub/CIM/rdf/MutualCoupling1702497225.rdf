@prefix cim: <http://iec.ch/TC57/2013/CIM-schema-cim16#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ont: <http://iec.ch/TC57/InformationModel#> .

ont:Transformer rdfs:subClassOf cim:Equipment .
ont:PowerTransformer rdfs:subClassOf ont:Transformer .

ont:MutualCoupling rdfs:subClassOf cim:ConductingEquipment .
ont:MutualCoupling rdfs:comment "Indicates that there is a mutual coupling between two conducting equipment." .

ont:MutualCoupling rdf:type ont:EquivalentEquipment .
ont:MutualCoupling rdf:type cim:Equipment .
ont:MutualCoupling rdf:type cim:ConductingEquipment .

ont:MutualCoupling cim:ConductingEquipment.BaseVoltage "132000"^^xsd:float .
ont:MutualCoupling cim:ConductingEquipment.normalHead "12.0"^^xsd:float .
ont:MutualCoupling cim:ConductingEquipment.normalR1 "1.0"^^xsd:float .
ont:MutualCoupling cim:ConductingEquipment.normalX1 "1.2"^^xsd:float .
ont:MutualCoupling cim:ConductingEquipment.phaseAngle "30.0"^^xsd:float .

ont:Transformer.Equipment.PowerTransformer ont:PowerTransformer .
ont:Transformer.Equipment.PowerTransformer cim:PowerTransformer.vectorGroup "Dyn5"^^xsd:string .