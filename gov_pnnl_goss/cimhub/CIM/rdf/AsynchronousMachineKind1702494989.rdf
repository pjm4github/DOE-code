@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix cim: <http://iec.ch/TC57/CIM100#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

cim:AsynchronousMachineKind rdf:type rdfs:Class ;
    rdfs:subClassOf cim:RotatingMachineKind .

cim:WindTurbine rdf:type rdfs:Class ;
    rdfs:subClassOf cim:AsynchronousMachineKind .

cim:HydroTurbine rdf:type rdfs:Class ;
    rdfs:subClassOf cim:AsynchronousMachineKind .

cim:SteamTurbine rdf:type rdfs:Class ;
    rdfs:subClassOf cim:AsynchronousMachineKind .