@prefix cim: <http://iec.ch/TC57/2013/CIM-schema-cim16#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://example.org/> .

ex:LinearShuntCompensator rdf:type cim:LinearShuntCompensator ;
    cim:IdentifiedObject.name "LSCompensator1" ;
    cim:ConductingEquipment.BaseVoltage ex:BaseVoltage1 ;
    cim:ConductingEquipment.Terminal ex:Terminal1 ;
    cim:LinearShuntCompensator.aVRDelay <gld:AVRTypeAVR> ;
    cim:LinearShuntCompensator.bPerSection 0.1 ;
    cim:LinearShuntCompensator.gPerSection 0.2 ;
    cim:LinearShuntCompensator.normalSections 10 ;
    cim:LinearShuntCompensator.sections 5 .

ex:BaseVoltage1 rdf:type cim:BaseVoltage ;
    cim:BaseVoltage.nominalVoltage 11000 ;
    cim:BaseVoltage.nominalVoltageMultiplier 1 ;
    cim:BaseVoltage.nominalVoltageUnit "volt" .

ex:Terminal1 rdf:type cim:Terminal ;
    cim:Terminal.ConductingEquipment ex:LinearShuntCompensator ;
    cim:Terminal.ConnectivityNode ex:ConnectivityNode1 .

ex:ConnectivityNode1 rdf:type cim:ConnectivityNode ;
    cim:ConnectivityNode.ConnectivityNodeContainer ex:Substation1 .

ex:Substation1 rdf:type cim:Substation ;
    cim:Substation.name "Substation1" .