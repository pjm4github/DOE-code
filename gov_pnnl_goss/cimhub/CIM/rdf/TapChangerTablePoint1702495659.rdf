@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM100#> .

cim:TapChangerTablePoint rdf:type rdfs:Class ;
    rdfs:subClassOf cim:IdentifiedObject .

cim:TapChangerTablePoint.Point := [
    rdf:type cim:TapChangerTablePoint ;
    cim:TapChangerTablePoint.xPosition "0"^^xsd:integer ;
    cim:TapChangerTablePoint.yPosition "1.0"^^xsd:float ;
    cim:TapChangerTablePoint.x "1.0"^^xsd:float ;
    cim:TapChangerTablePoint.y "0.0"^^xsd:float ;
] .