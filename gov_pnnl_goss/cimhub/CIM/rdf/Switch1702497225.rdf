@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM100#cim/> .

cim:Switch rdfs:subClassOf cim:ConductingEquipment ;
    rdfs:label "Switch" .

# Instance of Switch
cim:SW1 a cim:Switch ;
    cim:IdentifiedObject.mRID "123456789" ;
    cim:IdentifiedObject.name "Switch 1" ;
    cim:Equipment.EquipmentContainer cim:Substation1 .

# Relationships
cim:SW1 cim:Switch.normalOpen true .