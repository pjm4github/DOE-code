@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM100#> .

cim:TapChangerControl rdf:type cim:TapChangerControl ;
    cim:TapChangerControl.normalStep cim:TapChangerControlStep#Step0 ;
    cim:TapChangerControl.reachNormalStep cim:TapChangerControlStep#Step0 ;
    cim:TapChangerControl.targetStep cim:TapChangerControlStep#Step0 ;
    cim:TapChangerControl.settableStep cim:TapChangerControlStep#Step0 ;
    cim:TapChangerControl.localOnOffControl false ;
    cim:TapChangerControl.remoteOnOffControl false ;
    cim:TapChangerControl.tapChanger cim:TapChanger#1 ;
    cim:TapChangerControl.tculControlMode cim:TculControlModeKind#fixed ;

cim:TapChangerControlStep#Step0 rdf:type cim:TapChangerControlStep ;
    cim:TapChangerControlStep.step cim:TapStep#Step0.