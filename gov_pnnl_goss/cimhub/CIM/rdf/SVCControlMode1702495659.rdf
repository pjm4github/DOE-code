@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix cim: <http://iec.ch/TC57/2018/CIM-schema-cim16#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cim:SVCControlMode rdf:type rdfs:Class ;
  rdfs:subClassOf cim:RegulatingControlMode .

cim:SVCControlMode.SVCMode rdf:type rdfs:DatatypeProperty ;
  rdfs:range cim:SVCModeKind .

cim:SVCControlMode.SVCModeSetPoint rdf:type rdfs:DatatypeProperty ;
  rdfs:range xsd:float .

cim:SVCControlMode.SVCModeKind rdf:type rdfs:Class ;
  rdfs:subClassOf cim:Enumeration .