@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM100#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

cim:ACLineSegment rdf:type rdfs:Class ;
  rdfs:subClassOf cim:Conductor ;
  rdfs:label "ACLineSegment" .

cim:ACLineSegment.r1 rdf:type cim:ACLineSegment ;
  cim:IdentifiedObject.mRID "01" ;
  cim:IdentifiedObject.name "Mains Line" ;
  cim:ConductingEquipment.BaseVoltage 110000 ;
  cim:ConductingEquipment.Terminal 01 ;
  cim:Conductor.length 10 ;
  cim:Conductor.phaseCount 3 ;
  cim:Conductor.usage cim:ConductorUsageKind.synchronous ;
  cim:Equipment.Aggregate false ;
  cim:Equipment.normallyInService true ;
  cim:ConductingEquipment.BaseVoltage "cim:VoltageLevel.r1" .

cim:ConductorUsageKind.synchronous rdf:type rdfs:Class ;
  rdfs:label "synchronous" .

cim:VoltageLevel.r1 rdf:type cim:VoltageLevel ;
  cim:VoltageLevel.nominalVoltage 110000 ;
  cim:VoltageLevel.substation "cim:Substation.r1" ;
  cim:VoltageLevel.lowVoltageLimit 11000 ;
  cim:VoltageLevel.highVoltageLimit 132000 ;
  cim:VoltageLevel.BaseVoltage "cim:BaseVoltage.r1" .

cim:Substation.r1 rdf:type cim:Substation ;
  rdfs:label "Main Substation" ;
  cim:Substation.Region "cim:SubGeographicalRegion.r1" .

cim:BaseVoltage.r1 rdf:type cim:BaseVoltage ;
  cim:BaseVoltage.nominalVoltage 110000 ;
  cim:BaseVoltage.conductingEquipment "cim:ACLineSegment.r1" .