@prefix cim: <http://iec.ch/TC57/2013/CIM-schema-cim16#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cim:TransformerCoreAdmittance rdf:type cim:TransformerCoreAdmittance ;
    cim:BasePower '1000000' ;
    cim:Conductance '0.001' ;
    cim:Reactance '0.01' ;
    cim:TransformerEndInfo [ 
        rdf:type cim:TransformerEndInfo ;
        cim:phaseAngleClock '0' ;
        cim:transformerEnd cim:TransformerEnd#1 
    ] .

cim:TransformerEnd#1 a cim:TransformerEnd ;
    cim:ratedS '1000000' ;
    cim:ratedU '11000' ;
    cim:r '0.01' ;
    cim:x '0.1' ;
    cim:connectionKind cim:Wye ;
    cim:baseVoltage cim:BaseVoltage#1 .

cim:BaseVoltage#1 a cim:BaseVoltage ;
    cim:nominalVoltage '11000' ;
    cim:basePower '1000000' .