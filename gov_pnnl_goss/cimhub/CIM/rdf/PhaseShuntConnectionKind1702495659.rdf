@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cim: <http://iec.ch/TC57/CIM#> .

cim:PhaseShuntConnectionKind a rdfs:Class ;
    rdfs:label "PhaseShuntConnectionKind"@en ;
    rdfs:subClassOf cim:Kind ;
    rdfs:comment "A kind of phase shunt connection"@en .

cim:PhaseShuntConnectionKind.ON a rdfs:Resource, cim:PhaseShuntConnectionKind ;
    rdfs:label "ON"@en ;
    rdfs:comment "The shunt is normally connected"@en .

cim:PhaseShuntConnectionKind.OFF a rdfs:Resource, cim:PhaseShuntConnectionKind ;
    rdfs:label "OFF"@en ;
    rdfs:comment "The shunt is normally disconnected"@en .